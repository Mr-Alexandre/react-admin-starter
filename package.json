    "scripts": {
        "dev": "nodemon -r dotenv/config --watch 'server/**' -e ts --exec ts-node --project server/tsconfig.json server/development.ts",
        "build": "webpack --mode production --env NODE_ENV=production",
        "build:server": "tsc --project server/tsconfig.json",
        "start": "nodemon -r dotenv/config --exec ts-node --project server/tsconfig.json server/start.ts",
        "test": "jest --watchAll",
        "test:ci": "jest --ci",
        "api": "nodemon -r dotenv/config --watch '_mock-api-server/**' -e ts --exec ts-node --project _mock-api-server/tsconfig.json _mock-api-server/index.ts",
        "extract-translations": "i18next",
        "lint": "eslint --ext .ts,.tsx,.js,.jsx ./",
        "lint:fix": "eslint --ext .ts,.tsx,.js,.jsx ./ --fix",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "bundle-analyze": "webpack-bundle-analyzer -m json ./dist/report.json"
    },
